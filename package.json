  "private": true,
  "scripts": {
    "prebuild": "node scripts/internal/genVer.js",
    "preinstall": "npx use-yarn -f config/useYarn.txt",
    "extract-intl": "node scripts/internal/extract-intl.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "pretty-quick": "npx pretty-quick"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty-quick --staged && yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "src/*.+(json|css|md|scss)": [
      "prettier --write",
      "git add"
    ]
